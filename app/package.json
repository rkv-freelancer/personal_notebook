{
  "name": "desktop",
  "productName": "Surf",
  "version": "0.0.1",
  "main": "./out/main/index.js",
  "author": "Deta GmbH",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node",
    "start": "electron-vite preview",
    "dev": "node scripts/dev.js",
    "build:typecheck": "npm run typecheck && electron-vite build",
    "build": "electron-vite build",
    "build:frontend": "electron-vite build",
    "build:mwl": "npm run build && electron-builder -mwl --config build/electron-builder-config.js --arm64 --x64 --publish always",
    "build:mac": "npm run build && electron-builder --mac --config build/electron-builder-config.js --arm64 --x64 --publish always",
    "build:mac:x64": "npm run build && electron-builder --mac --config build/electron-builder-config.js --x64 --publish always",
    "build:mac:arm": "npm run build && electron-builder --mac --config build/electron-builder-config.js --arm64 --publish always",
    "build:lin:x64": "npm run build && electron-builder --linux --config build/electron-builder-config.js --x64 --publish always",
    "build:lin:arm": "npm run build && electron-builder --linux --config build/electron-builder-config.js --arm64 --publish always",
    "build:win:x64": "npm run build && electron-builder --win --config build/electron-builder-config.js --x64 --publish always"
  },
  "dependencies": {
    "@deta/backend": "*",
    "@deta/dragcula": "*",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@ghostery/adblocker-electron": "2.1.1",
    "@pdfslick/core": "^3.0.0",
    "@rollup/plugin-replace": "^5.0.7",
    "clsx": "^2.1.1",
    "cross-fetch": "^4.0.0",
    "d3-drag": "^3.0.0",
    "d3-selection": "^3.0.0",
    "electron-drag-click": "^1.0.5",
    "electron-updater": "^6.1.7",
    "iconify-icon": "^2.1.0",
    "sharp": "^0.33.5",
    "svelte-dnd-list": "^0.1.8",
    "svelte-portal": "^2.2.1"
  },
  "devDependencies": {
    "7zip-min": "^1.4.4",
    "@babel/plugin-transform-arrow-functions": "^7.25.9",
    "@deta/editor": "*",
    "@deta/services": "*",
    "@deta/types": "*",
    "@deta/ui": "*",
    "@deta/utils": "*",
    "@deta/web-parser": "*",
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/fuses": "^1.7.0",
    "@mateothegreat/svelte5-router": "^2.16.19",
    "@svelte-plugins/tooltips": "^3.0.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^18.17.5",
    "autoprefixer": "^10.4.19",
    "bits-ui": "^2.9.3",
    "debug": "^4.4.3",
    "electron": "39.2.4",
    "electron-builder": "^24.13.3",
    "electron-context-menu": "^3.6.1",
    "electron-vite": "^4.0.0",
    "eslint": "^8.47.0",
    "eslint-plugin-svelte": "^3.11.0",
    "mime-types": "^3.0.1",
    "openai": "^4.28.4",
    "postcss": "^8.4.38",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "rollup-plugin-license": "^3.5.3",
    "rollup-plugin-obfuscator": "^1.1.0",
    "rollup-pluginutils": "^2.8.2",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.0",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.2",
    "typescript": "^5.5.0",
    "unzipper": "^0.10.14",
    "uuid": "^9.0.1",
    "vite": "^7.1.11",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-markdown": "^2.2.0",
    "vite-plugin-node-polyfills": "^0.24.0"
  }
}
